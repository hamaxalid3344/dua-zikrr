{
  "rewrites": [{ "source": "/(.*)", "destination": "/index.html" }],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "s-maxage=0, max-age=0" }
      ]
    }
  ],

  "crons": [
    {
      "path": "/api/send-daily-notification",
      "schedule": "0 9 * * *"
    }
  ]
}
